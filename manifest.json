{
  "manifestVersion": 4,
  "id": "6566704a",
  "name": ".ilp Plugin",
  "version": "1.0.1",
  "main": "index.html",
  "host": {
    "app": "PS",
    "minVersion": "23.0.0"
  },
  "entrypoints": [
    {
      "id": "ilp_config_exporter",
      "label": {
        "default": "Export .ilp config",
        "zh": "导出.ilp配置"
      },
      "type": "panel",
      "minimumSize": {
        "width": 320,
        "height": 320
      },
      "maximumSize": {
        "width": 1000,
        "height": 1000
      },
      "preferredDockedSize": {
        "width": 320
      },
      "preferredFloatingSize": {
        "width": 320
      },
      "icons": [
        {
          "width": 23,
          "height": 23,
          "path": "icons/dark.png",
          "scale": [
            1,
            2
          ],
          "theme": [
            "lightest",
            "light"
          ],
          "species": [
            "chrome"
          ]
        },
        {
          "width": 23,
          "height": 23,
          "path": "icons/light.png",
          "scale": [
            1,
            2
          ],
          "theme": [
            "darkest",
            "dark",
            "medium"
          ],
          "species": [
            "chrome"
          ]
        }
      ]
    }
  ],
  "icons": [
    {
      "width": 48,
      "height": 48,
      "path": "icons/plugin.png",
      "scale": [
        1,
        2
      ],
      "theme": [
        "dark",
        "darkest",
        "medium",
        "lightest",
        "light",
        "all"
      ],
      "species": [
        "pluginList"
      ]
    }
  ],
  "requiredPermissions": {
    "network": {
      "domains": "all"
    },
    "allowCodeGenerationFromStrings": true
  }
}
